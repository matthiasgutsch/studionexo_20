{
  "version": 2,
  "name": "studionexo_20",
  "public": true,
  "buildCommand": "npm ci && npm run build:ssr",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.0",
      "includeFiles": "dist/studionexo_20/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/studionexo_20/browser/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}