{
  "version": 2,
  "name": "studionexo_20",
  "public": true,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.mjs"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/studionexo_20/browser/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.mjs"
    }
  ],
  "functions": {
    "api/index.mjs": {
      "runtime": "nodejs20.x",
      "includeFiles": "dist/studionexo_20/**"
    }
  }
}