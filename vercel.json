{
  "version": 2,
  "name": "studionexo_20",
  "public": true,
  "builds": [
    {
      "src": "dist/studionexo_20/server/main.server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/studionexo_20/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/studionexo_20/server/main.server.mjs"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/studionexo_20/**"
    }
  }
}